#include <iostream>
using namespace std;

int main() {
    int grade;

    cout << "Enter your grade (0â€“100): ";
    cin >> grade;

    if (grade >= 90)
        cout << "Your grade is A\n";
    else if (grade >= 80)
        cout << "Your grade is B\n";
    else if (grade >= 70)
        cout << "Your grade is C\n";
    else if (grade >= 60)
        cout << "Your grade is D\n";
    else
        cout << "Your grade is F\n";

    return 0;
}
